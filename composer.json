{
    "name": "hexlet/code",
    "description": "differences generator",
    "type": "library",
    "autoload": {
        "psr-4": {
            "Differ\\": "src/",
            "Differ\\Tests\\": "tests/"
        },
        "files": [
            "src/Differ/Differ.php",
            "src/Parsers.php",
            "src/Formatters/Stylish.php",
            "src/Formatters/Formatters.php",
            "src/Formatters/Plain.php"
        ]
    },
    "authors": [
        {
            "name": "Pavel Kondybailo",
            "email": "pavelkondybailo@yandex.by"
        }
    ],
    "require": {
        "docopt/docopt": "^1.0",
        "symfony/yaml": "^6.0"
    },
    "require-dev": {
        "squizlabs/php_codesniffer": "^3.6",
        "phpunit/phpunit": "^9.5"
    }
}
